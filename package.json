{
  "name": "latertots",
  "version": "0.1.0",
  "homepage": "https://www.latertotsdropin.com",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@faker-js/faker": "^9.7.0",
    "@firebase/rules-unit-testing": "^3.0.4",
    "@fullcalendar/core": "^6.1.17",
    "@fullcalendar/daygrid": "^6.1.11",
    "@fullcalendar/interaction": "^6.1.11",
    "@fullcalendar/list": "^6.1.11",
    "@fullcalendar/react": "^6.1.11",
    "@fullcalendar/timegrid": "^6.1.11",
    "@hookform/resolvers": "^5.1.1",
    "@mui/icons-material": "^5.15.15",
    "@mui/material": "^5.15.15",
    "@reduxjs/toolkit": "^2.2.7",
    "@tanstack/react-query": "^5.79.0",
    "@tanstack/react-query-devtools": "^5.79.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@welldone-software/why-did-you-render": "^8",
    "firebase": "^10.14.1",
    "firebaseui": "^6.1.0",
    "joi": "^17.13.3",
    "lodash": "^4.17.21",
    "luxon": "^3.7.1",
    "polished": "^4.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.2",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.22.3",
    "react-scripts": "5.0.1",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:emulator:react": "REACT_APP_USE_EMULATOR=true react-scripts start",
    "start:emulator:seed": "npm run emulator:seed && npm run start:emulator",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:auth": "react-scripts test --testPathPattern=auth",
    "test:coverage": "react-scripts test --coverage",
    "test:watch": "react-scripts test --watchAll",
    "eject": "react-scripts eject",
    "todo-check": "eslint --no-eslintrc -c .eslintrc-todo.json \"src/**/*.{js,jsx}\"",
    "emulator:start": "firebase emulators:start --config emulator/firebase.json --project latertots-a6694",
    "emulator:stop": "kill-port 4000 5001 8080 8085 9099 9199",
    "emulator:seed": "npm run emulator:seed:auth && npm run emulator:seed:firestore",
    "emulator:seed:auth": "node emulator/auth/auth-seed.mjs",
    "emulator:seed:firestore": "node emulator/firestore/seed.mjs",
    "emulator:export-seed": "ts=$(date +%Y%m%d%H%M%S) && mkdir -p \"./emulator/firestore/exports/$ts\" && npm run emulator:seed && firebase emulators:export \"./emulator/firestore/exports/$ts\" --only firestore"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "kill-port": "^2.0.1"
  }
}
